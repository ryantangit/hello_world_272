metadata:
  name: hello
spec:
  replicas: 1
  selector:
    matchLabels:
      app: hello
  template:
    metadata:
      labels:
        app: hello
  spec:
    containers:
      - name: hello
        image: ryantandocker/hello_272:latest
        ports:
          - containerPort: 4445
